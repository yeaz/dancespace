<%= render "home/navbar" %>
<div class="container spec">
<h1><b>All events</b></h1>
<% for e in @events do %>
	  <h2><b><a "<%= event_path(e) %>" data-no-turbolink="true"><%= e.name %></a></b></h2>
	  <div>Description: <%= e.description %></div>
	  <div>Created by: <a href="<%= studio_path(e.studio_id)%>"><%= e.studio.name %></a></div>
 	  <div>Date and time: <%= e.event_date_time %></div>
<% end %>
</div>