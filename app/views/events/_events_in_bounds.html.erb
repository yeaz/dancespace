<% if not events.empty? %>
  <% events.each do |e| %>
    <h3><a href="<%= event_path(e) %>" data-no-turbolink="true"><%= e.name %></a></h3>
    <div>Description: <%= e.description %></div>
    <div>Created by: <a href="<%= studio_path(e.studio_id)%>" data-no-turbolink="true"><%= e.studio.name %></a></div>
    <div>Date: <%= e.event_date %></div>
    <div>Time: <%= e.event_time %></div>
  <% end %>
<% end %>
