<div id="panel">
  <input id="events_feed_address" type="textbox" placeholder="Stanford, CA"/>
  <input type="submit" onclick="codeAddressAndUpdateEvents()" value="Change Location!"/>
</div>
<div id="events_feed_error"></div>

<div id="events-feed-map-canvas"></div>








	<% Event.find_each do |e| %>
					  <h3><a href="<%= event_path(e) %>"><%= e.name %></a></h3>
					  <div>Description: <%= e.description %></div>
					  <div>Created by: <a href="<%= studio_path(e.studio_id)%>"><%= e.studio.name %></a></div>
					  <div>Date and time: <%= e.event_date_time %></div>
					<% end %>
